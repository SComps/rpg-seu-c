PROC 1 DSN
  /********************************************************************/
  /* CLIST TO START THE SEU EDITOR                                    */
  /********************************************************************/
  IF &STR(&DSN) = HELP THEN DO
    WRITE USAGE: SEU 'DATASET.NAME(MEMBER)'
    EXIT
  END
  
  /* PASS DSN DIRECTLY TO THE CALL COMMAND */
  CALL 'HERC01.TEST.LOADLIB(SEU)' &DSN
