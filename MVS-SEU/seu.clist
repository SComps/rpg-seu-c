PROC 1 DSN
CONTROL MSG NOLIST
* ENSURE DD IS FREE
FREE FI(SYSASMEU)
* ALLOCATE DATASET - LET TSO HANDLE THE QUOTES IN &DSN
ALLOC FI(SYSASMEU) DA(&DSN) SHR REUSE
IF &LASTCC NE 0 THEN DO
  WRITE &STR(SEU: ALLOCATION FAILED FOR &DSN)
  EXIT CODE(8)
END
WRITE &STR(SEU: OPENING &DSN...)
CALL 'HERC01.TEST.LOADLIB(SEU)'
SET SAVERC = &LASTCC
WRITE &STR(SEU: SESSION ENDED RC=&SAVERC)
FREE FI(SYSASMEU)
